{
    "name": "@vgen-co/eslint-config",
    "version": "2.1.0",
    "description": "VGen's shared linting and formatting configs",
    "author": "VGen (https://vgen.co/)",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./prettier": {
            "import": "./dist/prettier.js",
            "require": "./dist/prettier.js"
        }
    },
    "scripts": {
        "artifactregistry-login": "npx google-artifactregistry-auth",
        "format": "npm run build && prettier --write .",
        "build": "tsc"
    },
    "peerDependencies": {
        "@eslint/js": "^9.24.0",
        "@stylistic/eslint-plugin": "^4.2.0",
        "eslint": ">= 9",
        "prettier": "^3.6.2",
        "typescript-eslint": "^8.30.1"
    },
    "devDependencies": {
        "typescript": "^5.8.3"
    }
}
